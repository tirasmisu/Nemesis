{
  "name": "tranium-bot-v5",
  "version": "5.0.0",
  "description": "A powerful moderation and utility Discord bot built with discord.js.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "guild": "node scripts/deploy-specific-commands.js",
    "global": "node scripts/update-commands.js",
    "updateperms": "node scripts/update-command-permissions-v14.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "startagain": "pm2 start index.js --name \"TraniumBotV5\"",
    "logs:rotate": "node scripts/logRotation.js",
    "logs:analyze": "node scripts/logRotation.js analyze",
    "logs:cleanup": "node scripts/logRotation.js 7",
    "performance": "node --expose-gc index.js",
    "monitor": "pm2 monit",
    "restart": "pm2 restart TraniumBotV5",
    "stop": "pm2 stop TraniumBotV5",
    "status": "pm2 status TraniumBotV5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tirasmisu/tranium-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "moderation",
    "utility"
  ],
  "author": "Eduardo Ordonez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tirasmisu/tranium-bot/issues"
  },
  "homepage": "https://github.com/tirasmisu/tranium-bot#readme",
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "bad-words-next": "^2.3.1",
    "dayjs": "^1.11.13",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "google-translate-api-x": "^10.7.2",
    "mongodb": "^6.8.0",
    "mongoose": "^7.8.1",
    "ms": "^2.1.3",
    "punycode": "^2.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.1"
  }
}
